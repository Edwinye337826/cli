{
  "name": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "description": "<%= description %>",
  "author": {
    "name": "<%= author %>"
  },
  "scripts": {
    "dev": "w6s-cli-script serve --open",
    "serve": "w6s-cli-script serve --open",
    "build": "w6s-cli-script build",
    "lint": "w6s-cli-script lint",
    "lint:style": "w6s-cli-script lint:style",
    "i18n:report": "w6s-cli-script i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "inspect": "w6s-cli-script inspect"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@w6s/codash": "^1.1.2",
    "@w6s/cordova-import": "^1.3.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "vant": "^2.8.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.17.5",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-module-decorators": "^0.17.0"
  },
  "devDependencies": {
    "@w6s/cli-script": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "w6s-cli-script lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
