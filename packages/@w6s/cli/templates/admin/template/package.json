{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "private": true,
  "version": "1.0.0",
  "author": "<%= author %>",
  "scripts": {
    "dev": "w6s-cli-script serve --open",
    "serve": "w6s-cli-script serve --open",
    "build": "w6s-cli-script build",
    "lint": "w6s-cli-script lint",
    "i18n:report": "w6s-cli-script i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "inspect": "w6s-cli-script inspect"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.9.2",
    "js-cookie": "^2.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.1.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.6",
    "vue-svgicon": "^3.2.6",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.9.9"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/nprogress": "^0.2.0",
    "@types/webpack-env": "^1.15.2",
    "@w6s/cli-script": "^2.0.0",
    "babel-core": "^6.26.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "w6s-cli-script lint",
      "git add"
    ]
  }
}
